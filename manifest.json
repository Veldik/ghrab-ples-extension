{
  "name": "ghrab-ples",
  "version": "1.0.0",
  "description": "Tento doplněk slouží k výpočtu volných míst u stolů na ghrabuvka plese.",
  "manifest_version": 3,
  "author": "Jakub Velička",
  "icons": {
    "16": "assets/icon16.png",
    "32": "assets/icon32.png",
    "48": "assets/icon48.png",
    "128": "assets/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://ples.ghrabuvka.cz/*"
      ],
      "js": [
        "main.js"
      ]
    }
  ]
}